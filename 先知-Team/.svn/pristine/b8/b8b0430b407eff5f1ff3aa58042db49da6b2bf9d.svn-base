package com.gem.xianzhi.pojo;

import java.util.Set;

public class User {
	private int id;
	private String loginId;
	private String pwd;
	private String nickName;
	private String headImg;//头像
	private String type;//普通用户，小编，大V，管理员
	private String state;//状态(正常，封禁)
	
	private UserDetail userDetail;
	
	private Set<Email>receiveEmails;//收到的邮件
	private Set<Email>sendEmails;//发送的邮件
	private Set<UserApply> fans;//粉丝(被关注)
	private Set<UserApply>followUser;//关注的人
	private Set<Complaint>complaint;//发起的投诉
	private Set<News>news;//发布的新闻
	private Set<News>collectNews;//收藏的新闻
	             
	public User(){}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getLoginId() {
		return loginId;
	}

	public void setLoginId(String loginId) {
		this.loginId = loginId;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	public String getNickName() {
		return nickName;
	}

	public void setNickName(String nickName) {
		this.nickName = nickName;
	}

	public String getHeadImg() {
		return headImg;
	}

	public void setHeadImg(String headImg) {
		this.headImg = headImg;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public UserDetail getUserDetail() {
		return userDetail;
	}

	public void setUserDetail(UserDetail userDetail) {
		this.userDetail = userDetail;
	}

	public Set<Email> getReceiveEmails() {
		return receiveEmails;
	}

	public void setReceiveEmails(Set<Email> receiveEmails) {
		this.receiveEmails = receiveEmails;
	}

	public Set<Email> getSendEmails() {
		return sendEmails;
	}

	public void setSendEmails(Set<Email> sendEmails) {
		this.sendEmails = sendEmails;
	}

	public Set<UserApply> getFans() {
		return fans;
	}

	public void setFans(Set<UserApply> fans) {
		this.fans = fans;
	}

	public Set<UserApply> getFollowUser() {
		return followUser;
	}

	public void setFollowUser(Set<UserApply> followUser) {
		this.followUser = followUser;
	}

	public Set<Complaint> getComplaint() {
		return complaint;
	}

	public void setComplaint(Set<Complaint> complaint) {
		this.complaint = complaint;
	}

	public Set<News> getNews() {
		return news;
	}

	public void setNews(Set<News> news) {
		this.news = news;
	}

	public Set<News> getCollectNews() {
		return collectNews;
	}

	public void setCollectNews(Set<News> collectNews) {
		this.collectNews = collectNews;
	}



}
